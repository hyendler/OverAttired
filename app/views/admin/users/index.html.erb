<h1>Hello from User Index Page</h1>
<h4> These are a list of all the users </h4>
<br>
<ul>
  <% @users.each do |user| %>
    <ul>
      <h3><%= user.first_name %> <%= user.last_name %></h3>
        <% if user.admin %>
        <b>adminstrator</b>
        <% end %>
      <ul>
        <li><%= user.email %></li>
      </ul>
    </ul>
  <% end %>
</ul>

<button type="button" class="btn btn-info btn-md"><%= link_to "Back to all products", admin_path %></button>
<button type="button" class="btn btn-default btn-md"><%= link_to "E-mail All Users Matches", send_updates_path %></button>